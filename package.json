{
  "name": "zliq",
  "version": "1.0.0",
  "description": "slim and quick framework in low loc",
  "main": "index.js",
  "scripts": {
    "test": "mocha-webpack --webpack-config webpack.tests.config.js \"test/**/*.spec.js\"",
    "tdd": "webpack-dev-server --config webpack.tests.config.js",
    "start": "webpack-dev-server --config webpack.config.js",
    "build": "webpack --config webpack.config.build.js",
    "build:min": "webpack -p --config webpack.config.build.js --output-filename=zliq.min.js --output-path=lib/min",
    "start:benchmark": "webpack-dev-server --config webpack.config.benchmark.js",
    "build:benchmark": "webpack --config webpack.config.benchmark.js",
    "build-prod": "npm run build:benchmark",
    "prepublish": "rimraf lib/** && npm run build && npm run build:min"
  },
  "author": "Fabian <faboweber@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "odiff": "0.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-h-jsx": "^1.0.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.24.0",
    "css-loader": "^0.26.1",
    "mocha": "^3.2.0",
    "mocha-loader": "^1.1.1",
    "mocha-webpack": "^0.7.0",
    "node-sass": "^4.5.0",
    "null-loader": "^0.1.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^5.0.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-node-externals": "^1.5.4",
    "worker-loader": "^0.8.0"
  },
  "repository": {
    "url": "https://github.com/faboweb/zliq.git",
    "type": "git"
  }
}
